#include<stdio.h>
#include<string.h>

char validarlogin(){

  char senha[5];
  char usuario[5];

  printf("Digite seu usu√°rio:);
  gets(usuario);
  printf("Digite sua senha:);
  gets(senha);

  printf("\n\n Para %s e %s", usuario, senha);

  if(strcmp(senha, "ASDFG") == 0 && strcmp(usuario, "David") == 0){

    printf("Acesso permitido!);

  }else{

    printf("Acesso negado!);

  }
}

int main (){

  validarlogin();
  return 0;
}